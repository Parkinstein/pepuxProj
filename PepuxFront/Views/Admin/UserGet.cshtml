@model IEnumerable<PepuxFront.IpServiceLink.Service>
<div class="contributors">
<div id="userblock" class ="userblock">
<button type="button" class=" btn-info" data-toggle="collapse" data-target="#demo">
    <span class="glyphicon glyphicon-collapse-up"></span> Свернуть
</button>
<div id="demo" class="collapse in">
<p>
    @*@Html.ActionLink("Create New", "Create")*@
</p>
<table class="table">
    <tr style="background-color: #337AB7; color: White">
        <th>
            @Html.Label("Login")
        </th>
        <th>
            @Html.Label("Email")
        </th>
        <th>
            @Html.Label("Группа")
        </th>
        <th>
            @Html.Label("User Name")
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr style="background-color: RGBA(250, 250, 250, 0.7)">
            <td>
                @Html.DisplayFor(modelItem => item.AdName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.Id})
            </td>*@
        </tr>
    }

</table>
    </div>

<script id="sessionJs" type="text/javascript">


    function GetData() {

        $.ajax(
            {
                type: "GET", //doing POST
                url: "/Admin/UserGet", //change URL as per your needs
                data: "{}",
                //contentType: "application/json; charset=utf-8", //Sending JSON
                dataType: "json",
                failure: function(msg) { alert(msg); }, //do something in case of failure
                success: function(msg) { alert(msg.d); } //do what you got to do on success
            }
        );
    }


    setTimeout(GetData, 3000); //change timeout duration here
</script>
<script>
    $(document).ready(function () {
        $("#demo").on("hide.bs.collapse", function () {
            $(".btn-info").html('<span class="glyphicon glyphicon-collapse-down"></span> Развернуть');
        });
        $("#demo").on("show.bs.collapse", function () {
            $(".btn-info").html('<span class="glyphicon glyphicon-collapse-up"></span> Свернуть');
        });
    });
</script>
    </div>
</div>